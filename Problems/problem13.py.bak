
'''A PROGRAM TO PRINT WORDS OF A STRING HAVING LETTER 'A' AT SECOND POSITION'''

strr = 'my name is sandeep'

arr = strr.split()

for word in arr:
    if word[1] == 'a':
        print(word)
        
        
 
 
 
def odd_even_product(list_of_numbers):
    
    if sum(list_of_numbers)%2 == 0:
        return sum([x*x for x in list_of_numbers])
    else:
        return sum([y*y*y for x in list_of_numbers])
        
   
print(odd_even_product([1,2,3,4]))